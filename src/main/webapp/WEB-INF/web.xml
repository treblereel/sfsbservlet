<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>TestServlet</servlet-name>
		<display-name>TestServlet</display-name>
		<description></description>
		<servlet-class>org.treblereel.sfsbservlet.servlet.TestServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SecondTestServlet</servlet-name>
		<display-name>SecondTestServlet</display-name>
		<description></description>
		<servlet-class>org.treblereel.sfsbservlet.servlet.SecondTestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TestServlet</servlet-name>
		<url-pattern>/TestServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SecondTestServlet</servlet-name>
		<url-pattern>/SecondTestServlet</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>/TestServlet</welcome-file>
	</welcome-file-list>
</web-app>
